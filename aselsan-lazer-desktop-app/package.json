{
  "name": "your-app-name",
  "version": "0.1.0",
  "description": "Simple boilerplate for developing an Electron app with React",
  "author": "your name",
  "build": {
    "appId": "react.electron",
    "win": {
      "icon": "build/icon.png"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "antd": "^4.20.6",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^1.2.0",
    "google-map-react": "^2.1.10",
    "mobx": "5.15.6",
    "mobx-persist": "0.4.1",
    "mobx-persist-store": "0.5.20",
    "mobx-react": "6.3.0",
    "mobx-react-lite": "2.2.2",
    "moment": "^2.29.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "reactjs-bottom-navigation": "^1.0.4",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "start-react": "react-app-rewired start",
    "build-react": "react-app-rewired build",
    "test-react": "react-app-rewired test --env=jsdom",
    "eject-react": "react-app-rewired eject",
    "build-electron": "electron-builder",
    "build": "yarn build-react && yarn build-electron",
    "start": "concurrently \"cross-env BROWSER=none yarn start-react\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-import": "1.13.0",
    "concurrently": "^5.3.0",
    "customize-cra": "0.9.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "react-app-rewired": "2.1.5",
    "wait-on": "^5.2.1"
  }
}
